<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/user_list_in_detail">

    <fragment
        android:id="@+id/nav_frag_follower"
        android:name="sidev.app.course.dicoding.bab3_modul3.appcommon.frag.UserListFrag">
        <argument
            android:name="owner"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="null"/>
        <argument
            android:name="type"
            app:argType="integer"
            android:defaultValue="-1"/>
        <argument
            android:name="source"
            app:argType="integer"
            android:defaultValue="0"/>
        <argument
            android:name="loadOnResume"
            app:argType="boolean"
            android:defaultValue="false"/>
    </fragment>

    <fragment
        android:id="@+id/nav_frag_following"
        android:name="sidev.app.course.dicoding.bab3_modul3.appcommon.frag.UserListFrag">
        <argument
            android:name="owner"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="null"/>
        <argument
            android:name="type"
            app:argType="integer"
            android:defaultValue="-1"/>
        <argument
            android:name="source"
            app:argType="integer"
            android:defaultValue="0"/>
        <argument
            android:name="loadOnResume"
            app:argType="boolean"
            android:defaultValue="false"/>
    </fragment>

</navigation>